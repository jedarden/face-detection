{
  "name": "face-detection-app",
  "version": "1.1.0",
  "description": "Face detection application using face-api.js",
  "main": "src/index.js",
  "scripts": {
    "dev": "webpack serve --mode development --config webpack.dev.js",
    "build": "node download-models.js && webpack --mode production --config webpack.prod.js",
    "build:simple": "node download-models.js",
    "start": "node server.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern='^((?!e2e).)*\\.test\\.js$'",
    "test:e2e": "jest --config=jest.e2e.config.js",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js"
  },
  "keywords": [
    "face-detection",
    "face-api",
    "computer-vision"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.19.2",
    "face-api.js": "^0.22.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.1.4",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.51.0",
    "html-webpack-plugin": "^5.5.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.4",
    "jest-junit": "^16.0.0",
    "jest-puppeteer": "^9.0.0",
    "playwright": "^1.54.1",
    "puppeteer": "^24.14.0",
    "style-loader": "^3.3.3",
    "wait-on": "^7.0.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^5.10.0"
  },
  "overrides": {
    "node-fetch": "^2.6.7"
  }
}
